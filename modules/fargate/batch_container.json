[
    {
        "name": "${container_name}",
        "image": "${image}",
        "memoryReservation": 3584,
        "cpu": 1536,
        "essential": true,
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-region": "${region}",
                "awslogs-stream-prefix": "batch",
                "awslogs-group": "${logs_group}"
            }
        },
        "environment": [
            {"name": "example", "value": "example"}
        ],
        "command": "${command}"
    }
]